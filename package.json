{
    "name": "ol-webpack",
    "version": "1.0.0",
    "description": "Example using OpenLayers with Webpack",
    "scripts": {
        "build": "npm run buildArcgis",
        "buildGeoserver": "webpack --config webpack.config.js --mode development --serverType geoserver",
        "buildArcgis": "webpack --config webpack.config.js --mode development",
        "start": "npm run startGeoserver",
        "startGeoserver": "webpack-dev-server --config webpack.config.js --mode development --serverType geoserver --watch",
        "startArcgis": "webpack-dev-server --config webpack.config.js --mode development --serverType arcgis --watch",
        "dev": "http-server",
        "test": "webpack-dev-server --config webpack.config.test.js --mode development --watch"
    },
    "devDependencies": {
        "@babel/core": "^7.4.0",
        "@babel/preset-env": "^7.4.2",
        "babel-cli": "^6.26.0",
        "babel-loader": "^8.0.5",
        "babel-plugin-import": "^1.11.0",
        "babel-plugin-react-html-attrs": "^2.1.0",
        "babel-preset-es2015": "^6.24.1",
        "chai": "^4.2.0",
        "css-loader": "^2.1.1",
        "file-loader": "^3.0.1",
        "karma": "^4.1.0",
        "karma-chai": "^0.1.0",
        "karma-mocha": "^1.3.0",
        "karma-webpack": "^3.0.5",
        "mocha": "^6.1.3",
        "style-loader": "^0.23.1",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "url-loader": "^1.1.2",
        "webpack": "^4.12.1",
        "webpack-cli": "^3.3.0",
        "webpack-command": "^0.3.0",
        "webpack-dev-server": "^3.2.1",
        "webpack-merge": "^4.2.1"
    },
    "dependencies": {
        "@esri/arcgis-to-geojson-utils": "^1.3.0",
        "bootstrap": "^4.3.1",
        "echarts": "^4.2.1",
        "esri-loader": "^2.9.1",
        "hmap-js": "^1.5.8",
        "jquery": "^3.3.1",
        "ol": "^5.1.2",
        "ol-echarts": "^1.3.6",
        "ol-ext": "^3.1.2",
        "popper.js": "^1.15.0",
        "proj4": "^2.5.0"
    }
}